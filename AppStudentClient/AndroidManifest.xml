<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.fclassroom.appstudentclient"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="11"
        android:targetSdkVersion="21" />

    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.READ_PHONE_STATE" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.access_network_state" />
    <uses-permission android:name="android.permission.CAMERA"/>
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.ACCESS_WIFI_STATE" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.SYSTEM_ALERT_WINDOW" />
    <uses-permission android:name="android.permission.RESTART_PACKAGES" />
    <uses-permission android:name="android.permission.RECORD_AUDIO" />
    <uses-permission android:name="android.permission.VIBRATE" />
    <uses-permission android:name="android.permission.CAMERA" />
    <uses-permission android:name="android.permission.FLASHLIGHT" />

    <application
        android:name="com.fclassroom.AppContext"
        android:allowBackup="true"
        android:icon="@drawable/pic"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity android:name="com.fclassroom.activity.StartActivity" />
        <activity android:name="com.fclassroom.activity.RegisterActivity" />
        <activity
            android:name="com.fclassroom.activity.EnterActivity"
            android:launchMode="singleTask" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.fclassroom.activity.HomeActivity"
            android:launchMode="singleTask" />
        <activity
            android:name="com.tencent.tauth.AuthActivity"
            android:launchMode="singleTask"
            android:noHistory="true" >
            <intent-filter>
                <action android:name="android.intent.action.VIEW" />

                <category android:name="android.intent.category.DEFAULT" />
                <category android:name="android.intent.category.BROWSABLE" />

                <data android:scheme="tencent1104307228" />
                <!-- 100380359 100381104 222222 -->
            </intent-filter>
        </activity>
        <activity
            android:name="com.tencent.connect.common.AssistActivity"
            android:configChanges="orientation|keyboardHidden"
            android:launchMode="singleTask"
            android:screenOrientation="portrait"
            android:theme="@android:style/Theme.Translucent.NoTitleBar" />
        <activity
            android:name="com.fclassroom.activity.BindActivity"
            android:label="BindActivity"
            android:launchMode="singleTask" />
        <activity
            android:name="com.fclassroom.activity.CheckinforActivity"
            android:label="@string/title_activity_checkinfor" />
        <activity
            android:name="com.fclassroom.activity.BindtelephoneActivity"
            android:label="@string/title_activity_bindtelephone" />
        <activity
            android:name="com.fclassroom.activity.NotebookActivity"
            android:label="@string/title_activity_notebook" />
        <activity
            android:name="com.fclassroom.activity.SettingActivity"
            android:label="@string/title_activity_setting" />
        <activity
            android:name="com.fclassroom.activity.DetailActivity"
            android:label="@string/title_activity_detail" />
        <activity
            android:name="com.fclassroom.activity.AddwrongtagActivity"
            android:label="@string/title_activity_addwrongtag" />
        <activity
            android:name="com.fclassroom.activity.DialogcheckanswerActivity"
            android:label="@string/title_activity_dialog" />
        <activity
            android:name="com.fclassroom.activity.RemarkActivity"
            android:label="@string/title_activity_remark" />
        <activity
            android:name="com.fclassroom.activity.DialognewbookActivity"
            android:label="@string/title_activity_dialognewbook"
            android:theme="@style/Theme.AppCompat.Light.Dialog" />
        <activity
            android:name="com.fclassroom.activity.SearchActivity"
            android:label="@string/title_activity_search" />
        <activity
            android:name="com.fclassroom.activity.UserInfoActivity"
            android:label="@string/title_activity_user_info" />
        <activity
            android:name="com.fclassroom.activity.FindActivity"
            android:label="@string/title_activity_find" />
        <activity
            android:name="com.fclassroom.activity.MessageActivity"
            android:label="@string/title_activity_message" />
        <activity
            android:name="com.fclassroom.activity.TopRankActivity"
            android:label="@string/title_activity_top_rank" />
        <activity
            android:name="com.fclassroom.activity.AchievementActivity"
            android:label="@string/title_activity_achievement" />
        <activity
            android:name="com.fclassroom.activity.AboutActivity"
            android:label="@string/title_activity_about" />
        <activity
            android:name="com.fclassroom.activity.FeedbackActivity"
            android:label="@string/title_activity_feedback" />
        <activity
            android:name="com.fclassroom.activity.CheckResultActivity"
            android:label="@string/title_activity_check_result" >
